{% extends 'base.html.twig' %}

{% block meta_description %}
    <meta name="description" content="L'agence Firstcom réunit toutes les compétences indispensables pour optimiser la présence des entreprises sur le web et assurer leur croissance digitale. ">
{% endblock %}

{% block title %}Méthode et process{% endblock %}

{% block body %}

	<section id="container1">
		<div class="row align-items-center justify-content-center">
			<div class="col-md-4 col-12">
				<img class="image-principale" src="{{ asset('uploads/images/agence-methode.webp') }}">
			</div>
			<div class="col-md-4 col-12">
				<div class="fil_ariane">
					<a href="{{ path('app_home') }}">FIRSTCOM</a>
					| 
					<a href="{{ path('app_agence') }}">L'AGENCE</a>
                    |
                    <span>MÉTHODE ET PROCESS</span>
