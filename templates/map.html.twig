{% block body %}
<script>
    function initMap() {
        var location = {lat: {{ latitude|default('43.29123624641244') }}, lng: {{ longitude|default('5.375854608132841') }}};
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 15,
            center: location
        });
        var marker = new google.maps.Marker({
            position: location,
            map: map
        });
    }
</script>
{% endblock %}

